    <function name="Payment" type="Integer" variable="TotalBill">
        <parameters>
            <parameter name="roombill" type="Integer" array="False"/>
            <parameter name="restaurantbill" type="Integer" array="False"/>
            <parameter name="PaymentArray" type="String" array="True"/>
            <parameter name="NoOfdays" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="PaymentMethod" type="Integer" array="False" size=""/>
            <declare name="Totalbill" type="Integer" array="False" size=""/>
            <assign variable="Totalbill" expression="(roombill*NoOfDays)+(restaurantbill*NoOfDays)"/>
            <output expression="&quot;The total room bill is :&quot;  &amp;roombill" newline="True"/>
            <output expression="&quot;The total restuarent bill is :&quot; &amp;restaurantbill" newline="True"/>
            <output expression="&quot;The total bill is :&quot; &amp;Totalbill" newline="True"/>
            <assign variable="PaymentMethod" expression="0"/>
            <while expression="(PaymentMethod!=1) &amp;&amp; (PaymentMethod!=2) &amp;&amp; (PaymentMethod!=3)">
                <output expression="&quot;Please select the payment method&#13;&#10;1.Cash&#13;&#10;2.Debit card&#13;&#10;3.Credit card&#13;&#10;Enter 1 or 2 or 3&quot;" newline="True"/>
                <input variable="PaymentMethod"/>
                <if expression="PaymentMethod==1">
                    <then>
                        <assign variable="PaymentArray[0]" expression="&quot;Cash&quot;"/>
                    </then>
                    <else>
                        <if expression="PaymentMethod==2">
                            <then>
                                <assign variable="PaymentArray[0]" expression="&quot;Debit Card&quot;"/>
                            </then>
                            <else>
                                <if expression="PaymentMethod==3">
                                    <then>
                                        <assign variable="PaymentArray[0]" expression="&quot;Credit Card&quot;"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;INVALID INPUT&quot;" newline="True"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </while>
            <output expression="&quot;Payment Successfull&quot;" newline="True"/>
        </body>
    </function>
