            <assign variable="BookingArray[0]" expression="name"/>
            <assign variable="BookingArray[1]" expression="phonenumber"/>
            <assign variable="BookingArray[2]" expression="address"/>
            <assign variable="BookingArray[3]" expression="email"/>
        </body>
    </function>
