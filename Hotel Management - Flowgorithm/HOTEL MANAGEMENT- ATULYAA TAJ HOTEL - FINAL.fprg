    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="DateArray" type="String" array="True" size="3"/>
            <declare name="CheckinDate" type="String" array="False" size=""/>
            <declare name="CheckinDateArray" type="String" array="True" size="5"/>
            <declare name="CheckoutDate" type="String" array="False" size=""/>
            <declare name="CheckoutDateArray" type="String" array="True" size="5"/>
            <declare name="NoOfDays" type="Integer" array="False" size=""/>
            <declare name="NoOfDaysArray" type="Integer" array="True" size="5"/>
            <declare name="BookingArray" type="String" array="True" size="5"/>
            <declare name="Name" type="String" array="False" size=""/>
            <declare name="NameArray" type="String" array="True" size="5"/>
            <declare name="PhoneNumber" type="String" array="False" size=""/>
            <declare name="PhoneNumberArray" type="String" array="True" size="5"/>
            <declare name="Address" type="String" array="False" size=""/>
            <declare name="AddressArray" type="String" array="True" size="5"/>
            <declare name="Email" type="String" array="False" size=""/>
            <declare name="EmailArray" type="String" array="True" size="5"/>
            <declare name="RoomInfoArray" type="String" array="True" size="2"/>
            <declare name="RoomType" type="String" array="False" size=""/>
            <declare name="RoomTypeArray" type="String" array="True" size="5"/>
            <declare name="RoomBill" type="Integer" array="False" size=""/>
            <declare name="RestaurantBill" type="Integer" array="False" size=""/>
            <declare name="PaymentArray" type="String" array="True" size="3"/>
            <declare name="TotalBillArray" type="Integer" array="True" size="5"/>
            <declare name="TotalBill" type="Integer" array="False" size=""/>
            <declare name="PaymentType" type="String" array="False" size=""/>
            <declare name="PaymentTypeArray" type="String" array="True" size="5"/>
            <declare name="Decision" type="String" array="False" size=""/>
            <assign variable="Decision" expression="&quot;yes&quot;"/>
            <declare name="RoomNumber" type="Integer" array="False" size=""/>
            <assign variable="RoomNumber" expression="0"/>
            <output expression="&quot;Welcome to Atulyaa Taj Hotel&quot;" newline="True"/>
            <while expression="(Decision==&quot;Yes&quot;)||(Decision==&quot;YES&quot;)||(Decision==&quot;yes&quot;)">
                <assign variable="NoOfDays" expression="DateCalculation(DateArray)"/>
                <assign variable="CheckinDate" expression="DateArray[0]"/>
                <assign variable="CheckoutDate" expression="DateArray[1]"/>
                <call expression="Booking(BookingArray)"/>
                <assign variable="Name" expression="BookingArray[0]"/>
                <assign variable="PhoneNumber" expression="BookingArray[1]"/>
                <assign variable="Address" expression="BookingArray[2]"/>
                <assign variable="Email" expression="BookingArray[3]"/>
                <assign variable="RoomBill" expression="RoomInformation(RoomInfoArray)"/>
                <assign variable="RoomType" expression="RoomInfoArray[0]"/>
                <assign variable="RestaurantBill" expression="Restaurant()"/>
                <assign variable="TotalBill" expression="Payment(RoomBill,RestaurantBill,PaymentArray,NoOfDays)"/>
                <assign variable="PaymentType" expression="PaymentArray[0]"/>
                <call expression="DataStoring(CheckinDate,CheckinDateArray,CheckoutDate,CheckoutDateArray,Name,NameArray,PhoneNumber,PhoneNumberArray,Address,AddressArray,Email,EmailArray,RoomType,RoomTypeArray,TotalBill,TotalBillArray,PaymentType,PaymentTypeArray,RoomNumber)"/>
                <assign variable="RoomNumber" expression="RoomNumber+1"/>
                <output expression="&quot;Your RoomNumber Is :&quot; &amp; RoomNumber" newline="True"/>
                <output expression="&quot;Do you Want To Book Another Room&#13;&#10;Enter Yes or No&quot;" newline="True"/>
                <input variable="Decision"/>
            </while>
            <comment text="ONLY FOR MANAGEMENT "/>
            <output expression="&quot;Do You Want To Display Customer's BookingDetails&#13;&#10;Enter Yes or No &quot;" newline="True"/>
            <input variable="Decision"/>
            <while expression="(Decision==&quot;Yes&quot;)||(Decision==&quot;YES&quot;)||(Decision==&quot;yes&quot;)">
                <call expression="Display(CheckinDateArray,CheckoutDateArray,NameArray,PhoneNumberArray,AddressArray,EmailArray,RoomTypeArray,TotalBillArray,PaymentTypeArray)"/>
                <output expression="&quot;Do You Want To Display Another Customer's BookingDetails&#13;&#10;Enter Yes or No &quot;" newline="True"/>
                <input variable="Decision"/>
            </while>
            <output expression="&quot; Thank you for your visit &quot;" newline="True"/>
        </body>
    </function>
    <function name="Booking" type="None" variable="">
        <parameters>
            <parameter name="BookingArray" type="String" array="True"/>
        </parameters>
        <body>
            <output expression="&quot;Please enter your personal details&quot;" newline="True"/>
